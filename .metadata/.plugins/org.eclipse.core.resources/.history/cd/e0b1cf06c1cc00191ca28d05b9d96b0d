class Counter{
	int count;
	public void increment(){
		count++;
	}
}


public class Main {
	public static void main(String[] args)throws Exception{
		 final Counter obj = new Counter();
		
		Thread t1 = new Thread(
					new Runnable() {
						
						public void run() {
							for(int i=0;i<100;i++){
								obj.increment();
							}
						}
					});
		
		t1.start();
		t1.join();
		
		System.out.println("Count: "+obj.count);

	}

}
