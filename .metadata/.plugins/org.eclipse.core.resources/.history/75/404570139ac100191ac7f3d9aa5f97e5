class A{
	int i,j;
	public String toString(){
		return i + " " +j;
	}
}


public class Main {

	public static void main(String[] args) {
		//Shallow
		A obj = new A();
		obj.i =5;
		obj.j = 6;
		System.out.println(obj);
		A obj1 = obj;
		obj1.i = 10;
		System.out.println(obj);
		System.out.println(obj1);
	}

}
